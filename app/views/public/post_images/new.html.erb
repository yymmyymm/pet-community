<h1>画像投稿</h1>
  <%= form_with model: @post_image do |f| %>
    <h4>画像</h4>
      <%= f.file_field :image, accept: "image/*" %>
    <h4>タイトル</h4>
      <%= f.text_field :title, placeholder:"タイトル" %>
    <h4>説明(任意)</h4>
      <%= f.text_area :caption, placeholder:"説明" %>
    <%= f.submit '投稿' %>
  <% end %>